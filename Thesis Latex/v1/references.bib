@InProceedings{Metronomy,
	Title                    = {Metronomy: A function-architecture co-simulation framework for timing verification of cyber-physical systems},
	Author                   = {L. Guo and Q. Zhu and P. Nuzzo and R. Passerone and A. Sangiovanni-Vincentelli and E. A. Lee},
